{
  "from": 0,
  "size":1000,
  "query": {
    "bool": {
      "must": {
        "bool": {
          "must": [
            {
              "match": {
                "projectId": {
                  "query": "#PROJECT_ID#",
                  "type": "phrase"
                }
              }
            },
            {
              "match": {
                "flowId": {
                  "query": "#FLOW_ID#",
                  "type": "phrase"
                }
              }
            },
            {
              "range":{
                "umsTs":{
                  "ge": "#START_TIME#",
                  "le": "#END_TIME#"
                }
              }
            }
          ]
        }
      }
    }
  },
  "sort": [
    {
      "umsTs": {
        "order": "asc"
      }
    }
  ]
}